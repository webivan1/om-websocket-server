{
  "name": "websocket",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run compile && npm run execute",
    "compile": "tsc --build",
    "execute": "cross-env NODE_ENV=production node ./build",
    "dev-build": "cross-env NODE_ENV=development node ./build",
    "dev": "cross-env NODE_ENV=development && ts-node-dev --respawn --transpileOnly ./src/index.ts"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/express-handlebars": "^3.1.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.7.21",
    "@types/mysql2": "types/mysql2",
    "@types/node": "^13.13.5",
    "@types/redis": "^2.8.21",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-redis": "^1.0.26",
    "@types/uuid": "^7.0.3",
    "@types/validate.js": "^0.11.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.4",
    "moment": "^2.25.3",
    "mongoose": "^5.9.15",
    "mysql2": "^2.1.0",
    "redis": "^3.0.2",
    "socket.io": "^2.4.0",
    "socket.io-redis": "^5.2.0",
    "uuid": "^8.0.0",
    "validate.js": "^0.13.1"
  }
}
