  "scripts": {
    "start": "yarn compile && yarn execute",
    "compile": "tsc --build",
    "execute": "cross-env NODE_ENV=production node ./build",
    "dev": "cross-env NODE_ENV=development && ts-node-dev --respawn --transpileOnly ./src/index.ts"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.5",
    "@types/socket.io": "^2.1.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "socket.io": "^2.3.0"
  }
}
